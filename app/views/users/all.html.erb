<% provide(:title, "Users") %>
<% @users.each do |user| %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for user %>
        <%= user.name %> 
        <% if logged_in? %>
        <h1> HI
        	<%= user.logged %>
        </h1>
        <% end %>
      </h1>
      <!--<h1>
      <%= user.email %>
      </h1>
  		-->
    </section>
  </aside>
</div>
<% end %>